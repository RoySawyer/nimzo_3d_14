msgid "Load options"
msgstr "Opsi beban"

#: main_14.cpp:200
msgid "Load 3d options"
msgstr "Muat opsi 3d"

#: main_14.cpp:202
msgid "Select .dxo file to load 3d options"
msgstr "Pilih file .dxo untuk memuat opsi 3d"

