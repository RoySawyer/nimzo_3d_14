msgid "Save options"
msgstr "Αποθήκευση επιλογών"

#: main_14.cpp:194
msgid "Save 3d options"
msgstr "Αποθήκευση επιλογών 3D"

#: main_14.cpp:196
msgid "Select .dxo file to save 3d options"
msgstr "Επιλέξτε το αρχείο .dxo για να αποθηκεύσετε τις επιλογές 3D"

#: main_14.cpp:198
msgid "Load options"
msgstr "Επιλογές φόρτωσης"

#: main_14.cpp:200
msgid "Load 3d options"
msgstr "Φόρτωση επιλογών 3D"

#: main_14.cpp:202
msgid "Select .dxo file to load 3d options"
msgstr "Επιλέξτε το αρχείο .dxo για να φορτώσετε τις επιλογές 3d"

#: main_14.cpp:204
msgid "Save options..  -  Save the current 3d options to a file."
msgstr "Αποθήκευση επιλογών.. - Αποθηκεύστε τις τρέχουσες επιλογές 3D σε ένα αρχείο."

#: main_14.cpp:206
msgid "Load options..  -  Load the current 3d options from a file."
msgstr "Φόρτωση επιλογών.. - Φόρτωση των τρεχουσών επιλογών 3d από ένα αρχείο."

#: main_14.cpp:208
msgid "Save the current 3d options to a file"
msgstr "Αποθήκευση των τρεχουσών επιλογών 3D σε ένα αρχείο"

